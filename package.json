{
  "name": "graphql-typegen",
  "version": "1.0.0",
  "description": "Type generator for graphQL on runtime",
  "license": "ISC",
  "author": "Seyam",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch --include \"./src/*\" ./src/index.ts",
    "start": "node ./dist/index.js",
    "build": "rimraf dist && tsc --build",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^8.1.3",
    "@graphql-tools/load": "^8.1.3",
    "@graphql-tools/merge": "^9.1.2",
    "express": "^5.1.0",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-resolvers": "^5.1.0",
    "@parcel/watcher": "^2.5.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
